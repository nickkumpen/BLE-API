
{% extends '::base.html.twig' %}

{% block title %}
    <title>Log in</title>
{% endblock %}

{% block body %}
    <h1>Log in!</h1>

    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}


    {{ form_start(form) }}
    {{ form_widget(form) }}
    <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">

    {{ form_end(form) }}

{% endblock %}