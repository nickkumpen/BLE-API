{% extends '::base.html.twig' %}

{% block title %}
    <title>Registration</title>
{% endblock %}

{% block body %}
    <h1>Join team awesome!</h1>


        {{ form_start(form) }}
            {{ form_row(form.username) }}
            {{ form_row(form.email) }}
            {{ form_row(form.plainPassword.first) }}
            {{ form_row(form.plainPassword.second) }}

            <button type="submit">Register</button>
        {{ form_end(form) }}

{% endblock %}
